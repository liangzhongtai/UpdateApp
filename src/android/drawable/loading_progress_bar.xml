<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="5dip" />
			<gradient android:startColor="#eeeeee"
				android:centerColor="#eeeeee" android:centerY="0.75"
				android:endColor="#eeeeee" android:angle="270" />
		</shape>
	</item>

	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="5dip" />
				<gradient android:startColor="#246bb3"
					android:centerColor="#246bb3" android:centerY="0.75"
					android:endColor="#246bb3" android:angle="270" />
			</shape>
		</clip>
	</item>

	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="5dip" />
				<gradient android:startColor="#246bb3"
					android:centerColor="#246bb3" android:centerY="0.75"
					android:endColor="#246bb3" android:angle="270" />
			</shape>
		</clip>
	</item>

</layer-list>